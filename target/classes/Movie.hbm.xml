<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="example.hibernate.entity.Movie" table="Movie_Master">
<id name="movieId" type="int" column="movie_id">

</id>
<property name="movieName" type="string" column="movie_name" />
<property name="movieDirector" type="string" column="movie_director" />
<property name="movieDuration" type="string" column="movie_duration" />
<property name="movieReleaseDate" type="string" column="movie_ReleaseDate" />

</class>
</hibernate-mapping>